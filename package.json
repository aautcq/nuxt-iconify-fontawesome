    "chalk": "~5.3.0",
    "cli-progress": "~3.12.0",
    "pathe": "~1.1.2",
    "yargs": "~17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~2.21.1",
    "@fortawesome/free-brands-svg-icons": "~6.5.2",
