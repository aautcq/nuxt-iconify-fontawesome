{
  "name": "@aautcq/nuxt-iconify-fontawesome",
  "type": "module",
  "version": "1.0.0-rc.1",
  "private": false,
  "description": "Use Font Awesome icons with popular Nuxt modules Nuxt Icon and NuxtUI",
  "author": "Adrien Autricque <adrien.autricque@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/aautcq/nuxt-iconify-fontawesome#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aautcq/nuxt-iconify-fontawesome.git"
  },
  "bugs": {
    "url": "https://github.com/aautcq/nuxt-iconify-fontawesome/issues"
  },
  "keywords": [
    "fontawesome",
    "iconify",
    "icons",
    "nuxt"
  ],
  "main": "dist/index.js",
